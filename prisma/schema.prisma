generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String @id @unique @default(uuid())
  name        String
  email       String
  created_at  DateTime
  @@map("user")
}
